exists A. forall B. 
(
    (
        ("{action=0}_{A}" & "{action=0}_{B}") | ("{action=1}_{A}" & "{action=1}_{B}") | ("{action=2}_{A}" & "{action=2}_{B}")
    ) 
    U 
    ( 
        (("{beverage=0}_{A}" & "{beverage=0}_{B}") | ("{beverage=1}_{A}" & "{beverage=1}_{B}") | ("{beverage=2}_{A}" & "{beverage=2}_{B}"))
        |
        (("{water=0}_{A}" & "{water=0}_{B}") | ("{water=1}_{A}" & "{water=1}_{B}") | ("{water=2}_{A}" & "{water=2}_{B}"))
        |
        !("{NO_water}_{A}" <-> "{NO_water}_{B}") 
        |
        !("{NO_output}_{A}" <-> "{NO_output}_{B}")
    )
)
